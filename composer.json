{
    "name": "trt/async-tasks-bundle",
    "type": "bundle",
    "description": "This bundle allows to dispatch asynchronous event in symfony2. It uses RabbitMQ  to send messages to the consumers..",
    "keywords": ["rabbitmq", "message", "symfony", "queue"],
    "homepage": "git@github.com:toretto460/resque-bundle.git",
    "authors": [
        {
            "name" : "Simone Di Maulo"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/symfony" : "~2.3",
        "symfony/monolog-bundle": "~2.3",
        "videlalvaro/php-amqplib": ">=2.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=3.7.27"
    },
    "autoload": {
        "psr-0": { "Trt\\AsyncTasksBundle": "" }
    },
    "target-dir": "Trt/AsyncTasksBundle",
    "license": "LGPL-2.1"
}
